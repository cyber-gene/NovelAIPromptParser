<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>NovelAIPromptParser</Title>
        <Authors>cybergene</Authors>
        <Description>Load NovelAI prompt and settings from image files </Description>
        <Copyright>Copyright (c) 2022 cybergene</Copyright>
        <PackageProjectUrl>https://github.com/cyber-gene/NovelAIPromptParser</PackageProjectUrl>
        <PackageLicenseUrl></PackageLicenseUrl>
        <RepositoryUrl>https://github.com/cyber-gene/NovelAIPromptParser</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageVersion>1.0.1</PackageVersion>
        <PackageReleaseNotes>add README.md</PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
      <PackageReference Include="SixLabors.ImageSharp" Version="2.1.3" />
      <None Include="../README.md" Pack="true" PackagePath="\" />
    </ItemGroup>

</Project>
